@model assigMVCPeople.Models.City

@{
    ViewData["Title"] = "Details";
}

<div class="card">
    <h1 class="card-header text-center">Details of the city</h1>
    <hr />
    <dl class="row">
        <dt class="col-sm-4">
            @Html.DisplayNameFor(model => model.CityName)
        </dt>
        <dd class="col-sm-12">
            @Html.DisplayFor(model => model.CityName)
        </dd>
        <dt class="col-sm-4">
            @Html.DisplayNameFor(model => model.ZipCode)
        </dt>
        <dd class="col-sm-12">
            @Html.DisplayFor(model => model.ZipCode)
        </dd>
        <dt class="col-sm-4">
            @Html.DisplayNameFor(model => model.Country)
        </dt>
        <dd class="col-sm-12">
            @Html.DisplayFor(model => model.Country.CountryName)
        </dd>
    </dl>
</div>
<div>
   @* <a asp-action="Edit" asp-route-id="@Model?.CountryId">Edit</a> |*@
    <a asp-action="Index" class="go-btn">Go back</a>
</div>

